{
  "version": 2,
  "builds": [
    {
      "src": "api/src/app.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/src/app.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
